        </svg>
      </a>
    </div>
    <div class="col-9-sm col-9 right-side">
      {% block navbar %}
          {% include 'partials/navbar.html.twig' %}
      {% endblock %}
    </div>
  </nav>
    {% block sidebar %}
      <nav id="sidebar" class="col-3-sm col-3">
        {% include 'partials/sidebar.html.twig' %}
      </nav>
    {% endblock %}

    {% block body %}
    <section id="body" class="col-9-sm col-9">
        <div id="overlay"></div>

        <div class="padding highlightable">
            <a href="#" id="sidebar-toggle" data-sidebar-toggle><i class="fa fa-2x fa-bars"></i></a>

            {% block topbar %}
              {% if config.plugins.breadcrumbs.enabled %}
                  {% include 'partials/breadcrumbs.html.twig' %}
              {% endif %}
            {% endblock %}

            {% block content %}{% endblock %}

            {% block footer %}
                {% if  github_link_position == 'bottom' %}
                {% include 'partials/github_note.html.twig' %}
                {% endif %}
            {% endblock %}

        </div>
        {% block navigation %}{% endblock %}
    </section>
    {% endblock %}
    {% block analytics %}
        {% if theme_config.google_analytics_code %}
        {% include 'partials/analytics.html.twig' %}
        {% endif %}
    {% endblock %}
 </body>
</html>
